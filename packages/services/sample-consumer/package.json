{
  "name": "@nusantara/sample-consumer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node dist/server.js",
    "start": "node dist/index.js",
    "test": "pnpm run build && node dist/index.js",
    "lint": "pnpm exec eslint --ext .ts 'src' --max-warnings 0",
    "eslint:fix": "pnpm exec eslint --ext .ts 'src' --fix",
    "format": "pnpm exec prettier --write 'src/**/*.{ts,md,json}'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@nusantara/shared": "workspace:*",
    "@nusantara/generated-client": "workspace:*",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "body-parser": "^1.20.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["@nusantara/generated-client"]
    }
  },
  "devDependencies": {
    "supertest": "^6.3.3",
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "@types/knex": "^0.16.1",
    "@types/body-parser": "^1.19.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.0.0"
  }
}
